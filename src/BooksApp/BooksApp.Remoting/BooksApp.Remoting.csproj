  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\BooksApp.Core\BooksApp.Core.csproj" />
    <ProjectReference Include="..\BooksApp.Domain\BooksApp.Domain.csproj" />
  </ItemGroup>

</Project>
